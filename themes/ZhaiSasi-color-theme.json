{
    "tokenColors": [{
        "settings": {
            "background": "#EAEAEA",
            "foreground": "#555",
            "findHighlight": "#39f",
            "gutterForeground": "#999",
        }
    }, {
        "name": "评论",
        "scope": "comment",
        "settings": {
            "foreground": "#999",
            "background": "",
        }
    }, {
        "name": "字符串单引号",
        "scope": "string.quoted.single",
        "settings": {
            "foreground": "#4b4",
        }
    }, {
        "name": "字符串",
        "scope": "string",
        "settings": {
            "foreground": "#5a5",
        }
    }, {
        "name": "正则字符串",
        "scope": "string.regexp",
        "settings": {
            "foreground": "#93f",
        }
    }, {
        "name": "常量",
        "scope": "constant",
        "settings": {
            "foreground": "#39f",
        }
    }, {
        "name": "内置常量",
        "scope": "support.constant",
        "settings": {
            "foreground": "#39f",
            "fontStyle": "bold",
        }
    }, {
        "name": "语言常量",
        "scope": "constant.language",
        "settings": {
            "foreground": "#39f",
            "fontStyle": "bold",
        }
    }, {
        "name": "其他常量(css颜色、用户自定义等)",
        "scope": "constant.other",
        "settings": {
            "foreground": "#39f",
        }
    }, {
        "name": "变量",
        "scope": "variable",
        "settings": {
            "foreground": "#e33",
        }
    }, {
        "name": "内置变量",
        "scope": "variable.language",
        "settings": {
            "foreground": "#e33",
            "fontStyle": "bold",
        }
    }, {
        "name": "成员属性(类变量)",
        "scope": "variable.other.member",
        "settings": {
            "foreground": "#e33",
        }
    }, {
        "name": "js$符",
        "scope": "variable.other.dollar.only.js",
        "settings": {
            "foreground": "#e33",
        }
    }, {
        "name": "函数调用",
        "scope": "variable.function",
        "settings": {
            "foreground": "#93f",
        }
    }, {
        "name": "内置函数调用",
        "scope": "support.function",
        "settings": {
            "foreground": "#93f",
            "fontStyle": "bold",
        }
    }, {
        "name": "函数命名",
        "scope": "entity.name.function",
        "settings": {
            "foreground": "#93f",
        }
    }, {
        "name": "类名命名",
        "scope": "entity",
        "settings": {
            "foreground": "#d90",
        }
    }, {
        "name": "形参",
        "scope": "variable.parameter",
        "settings": {
            "foreground": "#d90",
        }
    }, {
        "name": "关键字",
        "scope": "keyword",
        "settings": {
            "foreground": "#00f",
        }
    }, {
        "name": "运算符箭头 and",
        "scope": "keyword.operator",
        "settings": {
            "foreground": "#00f",
        }
    }, {
        "name": "定义保留字(function static)",
        "scope": "storage",
        "settings": {
            "foreground": "#00f",
        }
    }, {
        "name": "库",
        "scope": "support",
        "settings": {
            "foreground": "#e3e",
        }
    }, {
        "name": "继承类名",
        "scope": "entity.other.inherited-class",
        "settings": {
            "foreground": "#e3e",
        }
    }, {
        "name": "html标签名",
        "scope": "entity.name.tag",
        "settings": {
            "foreground": "#39f",
        }
    }, {
        "name": "CSS属性名",
        "scope": "entity.other.attribute-name",
        "settings": {
            "foreground": "#e00",
        }
    }, {
        "name": "无效的",
        "scope": "invalid",
        "settings": {
            "foreground": "#000",
            "background": "#DDB700",
            "fontStyle": "bold underline",
        }
    }, {
        "name": "用户未定义",
        "scope": "User-defined",
        "settings": {
            "foreground": "#000",
            "fontStyle": "bold underline",
        }
    }],
    "colors": {
        "activityBar.foreground": "#123", //活动栏前景色(例如用于图标)
        "activityBar.background": "#C0CCD3", //活动栏背景色
