        "activityBar.dropBackground": "#E7EBF1", ////活动栏项目的拖放反馈颜色。
        "badge.foreground":"#fff", //小图标颜色
        "badge.background":"#08e", //小图标背景色
        "scrollbar.shadow": "#C0CCD3", //滚动条滑块阴影，指示视图已滚动。
        "scrollbarSlider.activeBackground": "#C0CCD3", //单击时，滚动条滑块的背景色。
        "scrollbarSlider.background": "#C0CCD3", //滚动条滑块背景色。
        "scrollbarSlider.hoverBackground": "#C0CCD3", //悬停时的滚动条滑块背景色。
        "sideBar.background": "#E1E5EA", //侧栏背景色。
        "sideBar.foreground": "#334455", //侧栏前景颜色。侧栏是浏览器和搜索等视图的容器。
        "sideBar.border": "#C0CCD3", //分隔编辑器一侧的边栏边框颜色。
        "sideBarTitle.foreground": "#334455", //侧栏标题前景颜色。
        "sideBarSectionHeader.background": "#C0CCD3", //侧栏部分标题背景色。
        "sideBarSectionHeader.foreground": "#334455", //侧栏部分标题前景色。
        "sideBarSectionHeader.border": "#C0CCD3", //侧栏部分标题边框颜色。
        "statusBar.background": "#9ab", //标准状态栏背景色
        "statusBar.debuggingBackground": "#9ab", //调试程序时状态栏的背景色
        "statusBar.noFolderBackground": "#9ab", //没有打开文件夹时状态栏的背景色
        "editorCursor.foreground": "#000", //编辑器光标颜色
        "editorLineNumber.foreground": "#ccc", //行号颜色
        "editorLineNumber.activeForeground": "#789", //行号当前颜色
        "editorGutter.background": "#EAEAEA", //行数的背景颜色
        "editorWhitespace.foreground": "#cfcfcf", //空格和tab还有字符的颜色
        "editor.background": "#EAEAEA", //编辑区背景颜色
        "editor.foreground": "#000", //编辑区默认字体颜色
        "editor.inactiveSelectionBackground": "#cee", //非活动编辑器内选中文字颜色
        "editor.selectionBackground": "#cde", //选中文字颜色
        "editor.selectionForeground": "#f00", //高对比度选中文字颜色
        "editor.selectionHighlightBackground": "#cee", //于所选项相同的内容
        "editor.selectionHighlightBorder": "#789", //于所选项相同的内容的边框颜色
        "editorGroupHeader.tabsBackground": "#E7EBF1", //工作区顶部标签之外的颜色
        "editorGroupHeader.tabsBorder": "#C0CCD3", //标签底边线颜色
        "tab.border": "#E7EBF1", //工作区顶部多标签之间的间隙颜色
        "tab.inactiveBackground": "#E1E5EA", //非激活标签背景颜色
        "tab.inactiveForeground": "#334455", //非激活标签字体颜色
        "tab.activeBackground": "#C0CCD3", //当前标签背景颜色
        "tab.activeForeground": "#000", //当前标签字体颜色
        "tab.hoverBackground": "#C0CCD3", //鼠标悬浮的标签背景颜色
        "minimap.findMatchHighlight":"#789",//迷你地图匹配的内容背景色
		"list.activeSelectionBackground": "#C0CCD3", // 侧边栏文件列表选中时背景色
		"list.activeSelectionForeground": "#334455", // 侧边栏文件列表选中时前景色
		"list.inactiveSelectionBackground": "#C0CCD3", // 侧边栏文件列不是焦点时背景色
		"list.inactiveSelectionForeground": "#334455", // 侧边栏文件列不是焦点时前景色
		"list.focusBackground": "#C0CCD3", //焦点时背景色
		"list.focusForeground": "#334455", //焦点时前景色
		"list.hoverBackground": "#C0CCD3", //鼠标悬停时背景色
		"list.hoverForeground": "#334455", //鼠标悬停时前景色
    },
    "name": "ZhaiSasi"
}
